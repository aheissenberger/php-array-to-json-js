{
  "name": "php-array-to-json-js",
  "version": "0.0.0",
  "type": "module",
  "main": "./dist/php-array-to-json-js.umd.cjs",
  "module": "./dist/php-array-to-json-js.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/php-array-to-json-js.js",
      "require": "./dist/php-array-to-json-js.umd.cjs"
    }
  },
  "license": "bsd-2-clause",
  "description": "Create PHP Array from JSON or Javascript Object Notation Code.",
  "keywords": [
    "php",
    "json",
    "js",
    "array",
    "converter"
  ],
  "author": "Andreas Heissenberger <andreas@heissenberger.at>",
  "homepage": "https://github.com/aheissenberger/php-array-to-json-js",
  "repository": {
    "type": "git",
    "url": "https://github.com/aheissenberger/php-array-to-json-js.git"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "npm:publish": "vitest --run && tsc && vite build && yarn publish"
  },
  "devDependencies": {
    "@types/node": "^18.13.0",
    "@types/which": "^2.0.1",
    "typescript": "^4.9.3",
    "vite": "^4.1.0",
    "vite-plugin-dts": "^1.7.2",
    "vitest": "^0.28.4",
    "which": "^3.0.0"
  },
  "dependencies": {}
}
